{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "importsNotUsedAsValues": "error",
    "ignoreDeprecations": "5.0",
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "skipLibCheck": true,
    "strict": true,
    "paths": {
      "pixi-panel/*": ["./node_modules/pixi-panel/*"]
    }
  },
  "include": [
    "**/*.d.ts",
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.svelte",
    ".*.js",
    "**/*.js",
    "**/*.jsx",
    ".*.cjs",
    "**/*.cjs"
  ]
}
