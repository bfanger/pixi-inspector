{
  "name": "pixi-inspector",
  "version": "0.9.1",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "test": "npm run lint",
    "lint": "eslint --ext js,vue --max-warnings 0 src",
    "clean": "rm -rf build",
    "build": "webpack --mode production -p --progress",
    "build:clean": "npm run clean && webpack --mode production -p",
    "build:watch": "webpack --mode development --watch --display none"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "esnext",
      "plugin:vue/recommended",
      "@vue/prettier"
    ],
    "plugins": [
      "only-warn"
    ],
    "globals": {
      "chrome": true,
      "process": true
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/env"
      ]
    ],
    "plugins": [
      "transform-vue-jsx"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-esnext": "^4.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "node-sass": "^4.7.2",
    "pixi.js": "^5.1.1",
    "prettier": "^1.11.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.1.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "rxjs": "^6.2.1",
    "vue": "^2.5.13",
    "vue-rx": "^6.0.0"
  }
}
