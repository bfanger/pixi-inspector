{
  "name": "pixi-inspector",
  "version": "0.8.1",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server",
    "test": "npm run lint",
    "lint": "eslint --ext js,vue --max-warnings 0 src",
    "clean": "rm -rf build",
    "build": "webpack -p --progress",
    "build:clean": "npm run clean && webpack -p",
    "build:watch": "webpack -w --display none"
  },
  "prettier": {
    "printWidth": 100
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "recommended/esnext",
      "plugin:vue/recommended",
      "prettier"
    ],
    "plugins": [
      "vue",
      "only-warn"
    ],
    "globals": {
      "chrome": true,
      "process": true
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-vue-jsx"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.1",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-esnext": "^2.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-recommended": "^2.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.3.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "node-sass": "^4.7.2",
    "pixi.js": "^4.7.0",
    "prettier": "^1.11.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "rxjs": "^5.5.6",
    "vue": "^2.5.13",
    "vue-rx": "^5.0.0"
  }
}
